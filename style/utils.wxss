/* 动画 */

.u-animationFadeUp {
  animation: animationFadeUp 0.5s;
}


.u-animationFadeDown {
  animation: animationFadeDown 0.5s;
}

@keyframes animationFadeUp {
  from {
    opacity: 0.5;
    transform: translateY(10rpx);
  }
  to {
    opacity: 1;
    transform: translateY(0);
  }
}


@keyframes animationFadeDown {
  from {
    opacity: 0.5;
    transform: translateY(-10rpx);
  }
  to {
    opacity: 1;
    transform: translateY(0);
  }
}

