page {
  padding-right: 0 !important;
}

.__item {
  position: relative;
  width: 45%;
  height: 300rpx;
  padding: 32rpx 32rpx;
  margin-bottom: 32rpx;
  margin-right: 32rpx;
  font-size: 34rpx;
  color: white;
  box-sizing: border-box;
  border-radius: 10rpx;
}

.__item:nth-child(1) {
  background-color: #339999;
}

.__item:nth-child(2) {
  background-color: #333333;
}

.__item:nth-child(3) {
  background-color: #99CCCC;
}

.__item:nth-child(4) {
  background-color: #0099CC;
}


.__item text:nth-of-type(1) {
  margin-bottom: 10rpx;
  font-size: 40rpx;
  color: white;
}

.__num {
  margin-top: 10rpx;
  font-size: 28rpx;
  color: white;
}
